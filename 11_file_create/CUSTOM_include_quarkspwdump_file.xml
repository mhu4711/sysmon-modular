<Sysmon schemaversion="4.30">
	<EventFiltering>
		<RuleGroup name="" groupRelation="or">
			<FileCreate onmatch="include">
				<!-- Detects a dump file written by QuarksPwDump password dumper -->
				<!-- Ref: https://raw.githubusercontent.com/Neo23x0/sigma/master/rules/windows/sysmon/sysmon_quarkspw_filedump.yml -->
				<TargetFilename condition="contains">\AppData\Local\Temp\SAM-</TargetFilename>
			</FileCreate>
		</RuleGroup>
	</EventFiltering>
</Sysmon>